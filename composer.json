    "type": "symfony-bundle",
    "description": "Single-sign-on bundle for Symfony2. Service Provider part.",
    "keywords": ["single-sign-on", "sso", "login", "otp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Kruithof",
            "email": "peter@financial-media.nl"
        },
        {
            "name": "Dmitry Korotovsky",
            "email": "dmitry@korotovsky.io"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-openssl": "*",
        "doctrine/orm": "~2.3",
        "symfony/framework-bundle": "~4.0",
        "symfony/dependency-injection": "~4.0",
        "korotovsky/sso-library": "dev-sf4-0.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.4",
        "symfony/phpunit-bridge": ">=2.2"
    },
    "autoload": {
        "psr-4": {
            "Krtv\\Bundle\\SingleSignOnServiceProviderBundle\\": "src/Krtv/Bundle/SingleSignOnServiceProviderBundle",
            "Krtv\\Bundle\\SingleSignOnServiceProviderBundle\\Tests\\": "tests/Krtv/Bundle/SingleSignOnServiceProviderBundle/Tests"
        }
    }
